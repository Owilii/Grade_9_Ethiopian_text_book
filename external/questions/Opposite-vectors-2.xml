p1y: rand_with_step(-5, -1, 1);
p2x: p1x+rand_with_step(1, 4, 1);
p2y: p1y+rand_with_step(1, 3, 1);
v1: [p2x-p1x, p2y-p1y];

p3x: 0;
p3y: 0;
p4x: p3x+rand_with_step(-5, -1, 1);
p4y: p3y+rand_with_step(1, 5, 1);

p1: [p1x, p1y];
p2: [p2x, p2y];
p3: [p3x, p3y];
p4: [p4x, p4y];

ta1: [[p2x+1, p2y], -v1]</text>
